{% if ((DEV.devmode == "on") and ((acces.admin.bit b-and USER.membre_accessite)) > 0) %}
<ul id="barredev">
	<li class="comment">Barre développement</li>
	<li id="refresh" class="" title="Rafraîchit la page, revient à la page d'accueil"><span><a href="index.php">Refresh</a></span></li>
	<li id="reinit" class="" title="Réinitialise le site, revient à la page d'accueil"><span><a href="index.php?init=init">Réinit</a></span></li>
	<li id="devdata" class="devreact" title="Données de développement"><span>Dev data</span>
		<ul>
			<li id="comment" class="devreact" title="Liste des commentaires placés sur AequaRENDER"><span>Comments</span></li>
		</ul>
	</li>
	<li id="stat0" class="devreact" title="Afficher données statistiques"><span>Aestat</span>
		<ul>
			<li id="stat1" class="" title="Purger les données statistiques"><span>Purger BDD stat complète</span></li>
			<li id="stat2" class="" title="Purger les données statistiques de mon IP"><span>Purger stat de mon IP</span></li>
		</ul>
	</li>
	<li id="devuser" class="devreact" title="Statut utilisateur"><span>User</span></li>
	<li id="devextall" class="devreact" title="Éléments externes"><span>Fichiers chargés</span>
		<ul>
			{% for key, item in ext %}
			<li id="devext{{ key }}" class="devreact"><span>Fichiers {{ key }}</span></li>
			{% endfor %}
		</ul>
	</li>
	<li title="Mises à jour ALL"><span><a href="?MAJALL=true&NOSTAT=true">MàJ ALL</a></span>
		<ul>
			{% for key, item in ext %}
			<li id="majext{{ key }}" class=""><span><a href="?MAJ{{ key }}=true&NOSTAT=true">MàJ {{ key }}</a></span></li>
			{% endfor %}
			<li id="majPHP" class=""><span><a href="?MAJPHP=true&NOSTAT=true">MàJ fichiers PHP</a></span></li>
			<li id="majAE" class=""><span><a href="?MAJAE=true&NOSTAT=true">MàJ AequaRENDER</a></span></li>
		</ul>
	<li id="twigD" class="devreact" title="Voir toutes les variables twig SSDATA"><span>SSDATA</span>
		<ul>
		    {% for key, value in _context %}{% if key != "_parent" %}
			<li id="twigD{{ key }}" class="devreact" title="Voir le détail de {{ key }}"><span>Variable {{ key }}</span></li>
		    {% endif %}{% endfor %}
		</ul>
	</li>
</ul>
{% include 'devdata.htpl' ignore missing %}
{% endif %}