ul#barredev {
	margin: 0px;
	padding: 0px;
	width: 100%;
	height: 22px;
	background-color: maroon;
	list-style-type: none;
	cursor: pointer;
	border-bottom: 1px solid orange;
}
ul#barredev * {
	margin: 0px;
	padding: 0px;
}
ul#barredev li.comment {
	color:yellow;
	cursor: default;
}
ul#barredev li:hover, ul#barredev li:hover span {
	color: white;
}
ul#barredev span {
	display:inline-block;
	width:100%;
	height:100%;
}
ul#barredev >li {
	height: 22px;
	float: left;
	margin: 0px 8px;
	color:#ccccff;
	line-height: 22px;
	font-size: 14px;
}
ul#barredev >li >span >a, ul#barredev >li >span {
	display: inline-block;
	height: 22px;
	color:orange;
	line-height: 22px;
	font-size: 14px;
}
ul#barredev >li >ul {
	display: none;
	position: absolute;
	background-color: #333333;
	list-style-type: none;
	border:1px solid orange;
	border-top:none;
}
ul#barredev >li >ul >li {
	padding: 1px 8px;
	display: block;
}
ul#barredev >li >ul >li >span >a, ul#barredev >li >ul >li >span {
	color: white;
	display: inline-block;
	height: 22px;
	line-height: 22px;
	font-size: 14px;
}
ul#barredev >li >ul >li:hover {
	background-color: #555555;
	color: white;
}
ul#barredev a:hover {
	color: white;
}

div.divcorr {
	color:#333333;
	font-size: 10px;
}
div.divcorr span {#cccccc
	color:#333;
}
div.divcorr span.normal {
	color:#888;
}
div.divcorr span.important {
	color:orange;
}
div.divcorr span.error {
	color:red;
}
