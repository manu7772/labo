<tr class="ajxu-{{ bundle }}Bundle{{ entite }}{{ ligne.id }}" data-prototype="{{ url('listeEntiteLigne', {'entite': bundle ~ 'Bundle' ~ entite, 'id': ligne.id}) }}">
	{% include "AcmeGroupLaboBundle:tableau:col_texteCentre.html.twig" with {"texte": ligne.id} %}
	{% include "AcmeGroupLaboBundle:tableau:col_actions.html.twig" %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"obj": ligne, "texte": 'nom'} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"obj": ligne, "texte": 'descriptif', "short": 50} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"obj": ligne, "texte": 'videoUrl'} %}
	{% include "AcmeGroupLaboBundle:tableau:col_date.html.twig" with {"date": ligne.dateCreation} %}
	{% include "AcmeGroupLaboBundle:tableau:col_date.html.twig" with {"date": ligne.dateMaj} %}
	{% include "AcmeGroupLaboBundle:tableau:col_statut.html.twig" with {"statut": ligne.statut} %}
</tr>
