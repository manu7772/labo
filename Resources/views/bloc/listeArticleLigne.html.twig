<tr class="ajxu-{{ bundleNameEntiteName }}{{ ligne.id }}" data-prototype="{{ url('listeEntiteLigne', {'classEntite': bundleNameEntiteName, 'id': ligne.id}) }}">
	{% include "AcmeGroupLaboBundle:tableau:col_texteCentre.html.twig" with {"texte": ligne.id} %}
	<td>{% if ligne.imagePpale != null %}
		<a href="{{ asset('images/original/' ~ ligne.imagePpale.fichierNom) }}" class="fancybox" title="{{ ligne.imagePpale.nom }}">
			<img src="{{ asset('images/tn64/' ~ ligne.imagePpale.fichierNom) }}" width="32" height="32" class="img-thumbnail" />
		</a>{% endif %}
	</td>
	{% include "AcmeGroupLaboBundle:tableau:col_actions.html.twig" %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"ligne": ligne, "value": nom'} %}
	{% set TVA = ligne.prix * ligne.tauxTVA.taux / 100 %}
	{% include "AcmeGroupLaboBundle:tableau:col_euro.html.twig" with {"obj": ligne, "texte": 'prixHT'} %}
	{% include "AcmeGroupLaboBundle:tableau:col_taux.html.twig" with {"taux": ligne.tauxTVA.taux} %}
	{% include "AcmeGroupLaboBundle:tableau:col_euro.html.twig" with {"euro": TVA} %}
	{% include "AcmeGroupLaboBundle:tableau:col_euro.html.twig" with {"obj": ligne, "texte": 'prix'} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texteCentre.html.twig" with {"texte": ligne.ventes} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texteCentre.html.twig" with {"texte": ligne.rank} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texteCentre.html.twig" with {"texte": ligne.notation} %}
	{% include "AcmeGroupLaboBundle:tableau:col_date.html.twig" with {"date": ligne.dateCreation} %}
	{% include "AcmeGroupLaboBundle:tableau:col_date.html.twig" with {"date": ligne.dateMaj} %}
	{% include "AcmeGroupLaboBundle:tableau:col_date.html.twig" with {"date": ligne.dateExpiration} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"obj": ligne, "texte": 'refFabricant'} %}
	{% include "AcmeGroupLaboBundle:tableau:col_texte.html.twig" with {"texte": "---"} %}
	{% include "AcmeGroupLaboBundle:tableau:col_statut.html.twig" with {"statut": ligne.statut} %}
</tr>
